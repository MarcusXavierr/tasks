{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "format": "prettier --write \"resources/**/*.{js,vue,css,html}\" \"*.{js,json}\"",
        "format:check": "prettier --check \"resources/**/*.{js,vue,css,html}\" \"*.{js,json}\"",
        "lint": "eslint resources/js --ext .js,.vue",
        "lint:fix": "eslint resources/js --ext .js,.vue --fix",
        "check": "npm run lint && npm run format:check"
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@inertiajs/vue3": "^2.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "autoprefixer": "^10.4.12",
        "axios": "^1.8.2",
        "concurrently": "^9.0.1",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-prettier": "^5.4.1",
        "eslint-plugin-vue": "^10.2.0",
        "laravel-vite-plugin": "^1.2.0",
        "postcss": "^8.4.31",
        "prettier": "^3.5.3",
        "tailwindcss": "^3.2.1",
        "vite": "^6.2.4",
        "vue": "^3.4.0"
    },
    "dependencies": {
        "@inertiajs/vue3": "^2.0.12",
        "@vitejs/plugin-vue": "^5.2.4",
        "vue": "^3.5.16"
    }
}
